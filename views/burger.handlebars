{{#totalresetwait}}{{/totalresetwait}}
{{#falsyresetwait}}{{/falsyresetwait}}
{{#totalreseteaten}}{{/totalreseteaten}}
{{#falsyreseteaten}}{{/falsyreseteaten}}
<div class="container">
    <div class="row"></div>
    <div class="row">
        <div class="card blue-grey darken-1">
            <div id="w-card" class="card-content white-text">
                <div id="w-card-title" class="card-action">
                    <img id="burguer-img" src="/assets/img/burger.jpg" width="200px" height="200px">
                </div>
                <div id="w-cardrow" style="border: 0px;" class="row">
                    <div id="w-cardrow-cont" class="col s12 m4 l2">
                        <a id="w-btn-submit" class="waves-effect waves-light orange darken-4 btn-large">Submit</a>
                    </div>
                    <div id="w-cardrow-cont" class="col s12 m4 l10">
                        <div class="input-field col s12">
                            <input id="order" type="text" class="validate">
                            <label for="order">Order</label>
                        </div>
                    </div>
                </div>
                <div id="w-cardrow" class="row">
                    <div id="w-cardrow-cont" class="col s12 m4 l6">
                        <h5 id="w-lbl-wait">Wait List</h5>
                        <ul id="w-coll-wait" class="collection">
                            {{#each array}}
                                {{#totalcountwait}}{{/totalcountwait}}
                                {{#if this.ID1}}
                                    <li id="w-coll-wait-item" class="collection-item">
                                        <div>{{ID1}}
                                            <a href="#!" class="secondary-content">
                                                <i id="icon-eat" class="fas fa-utensils"></i>
                                            </a>
                                        </div>
                                    </li>
                                {{/if}}
                                {{#unless this.ID1}}
                                    {{#falsycountwait}}{{/falsycountwait}}
                                {{/unless}}
                            {{/each}}
                            {{#fullfalseywait}}
                                <li id="w-coll-wait-item" class="collection-item">No Food is on the waitlist</li>
                            {{/fullfalseywait}}                            
                        </ul>
                    </div>
                    <div id="w-cardrow-cont" class="col s12 m4 l6">
                        <h5 id="w-lbl-eat">Eaten</h5>
                        <ul id="w-coll-eat" class="collection">
                            {{#each array}}
                                {{#totalcounteaten}}{{/totalcounteaten}}
                                {{#if this.ID2}}
                                    <li id="w-coll-eat-item" class="collection-item">{{ID2}}</li>
                                {{/if}}
                                {{#unless this.ID2}}
                                    {{#falsycounteaten}}{{/falsycounteaten}}
                                {{/unless}}
                            {{/each}}
                            {{#fullfalseyeaten}}
                                <li id="w-coll-eat-item" class="collection-item">No Food has been eaten</li>
                            {{/fullfalseyeaten}}
                        </ul>
                    </div>
                </div>
                <div id="w-cardrow" style="text-align: right;" class="row">
                    <div id="w-cardrow-cont" class="col s12 m4 l9"></div>
                    <div id="w-cardrow-cont" class="col s12 m4 l3">
                        <a id="w-btn-clear" class="waves-effect waves-light btn-large">Clear All</a>
                    </div>
                </div>
            </div>
        </div>    
    </div>
    <div class="row"></div>
    <div class="row"></div>
</div>